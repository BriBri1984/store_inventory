{% if items is not empty %}

            <table class="table">
                <thead class="thead-inverse">
                <tr>
                    <th>#</th>
                    <th>Product Name</th>
                    <th>Store</th>
                    <th>Stock Quantity</th>

                </tr>
                </thead>
                <tbody>
                {% for item in items %}
                    <tr>
                        <th scope="row">{{ item.id }}</th>
                        <td>{{ item.stock.name }}</td>
                        <td>{{ item.store.storeName }}</td>
                        <td>{{ item.quantity }}</td>


                    </tr>
                {% endfor %}

                </tbody>
            </table>
        {% endif %}